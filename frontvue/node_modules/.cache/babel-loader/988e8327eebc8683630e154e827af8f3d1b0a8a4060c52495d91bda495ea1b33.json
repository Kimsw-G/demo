{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  components: {},\n  data() {\n    return {\n      dows: [\"SUN\", \"MON\", \"TUE\", \"WED\", \"THU\", \"FRI\", \"SAT\"],\n      year: 0,\n      month: 0,\n      firstDay: 0,\n      lastDay: 0,\n      preLastday: 0,\n      dates: []\n    };\n  },\n  created() {\n    this.init();\n  },\n  mounted() {},\n  unmounted() {},\n  methods: {\n    init(param) {\n      if (param) {\n        // 다른 날짜로 이동시 \n        this.year = param[0];\n        this.month = param[1];\n        // this.calendarDate()\n      } else {\n        // 초기 화면 렌더링시\n        const date = new Date();\n        this.year = date.getFullYear();\n        this.month = date.getMonth() + 1;\n\n        // this.calendarDate()\n      }\n    },\n\n    getFirstDayLastDate(year, month) {\n      // 1일의 요일 및 마지막 날짜 구하기\n      const firstDay = new Date(year, month - 1, 1).getDay(); // 1일의 요일\n      const lastDate = new Date(year, month, 0).getDate(); // 마지막 날짜\n\n      let lastMonth = month - 1;\n      if (month === 1) {\n        lastMonth = 12;\n        year -= 1;\n      }\n      const prevLastDate = new Date(year, lastMonth, 0).getDate(); // 저번달의 마지막 날짜\n      return [firstDay, lastDate, prevLastDate];\n    },\n    setDayDate() {\n      let dayCnt = 1;\n      let nextDayCnt = 1;\n      for (let i = 0; i < 6; i++) {\n        let tmp;\n        for (let j = 0; j < 7; j++) {\n          if (i == 0 && j < this.firstDay) {\n            // 첫줄에서 저번 달 표시\n            tmp[j] = this.preLastday - this.firstDay + 1;\n          } else if (dayCnt > this.lastDate) {\n            // 마지막 줄에서\n            tmp[j] = nextDayCnt++;\n          } else {\n            tmp[j] = dayCnt++;\n          }\n        }\n        this.dates.push(tmp);\n      }\n    },\n    setDayData() {\n      let tmp = this.getFirstDayLastDate(this.year, this, this.month);\n      this.firstDay = tmp[0];\n      this.lastDay = tmp[1];\n      this.preLastday = tmp[2];\n    }\n  }\n};","map":{"v